        "watch-tests": "cross-env BABEL_ENV=test jest --watch",
        "prepare": "husky install",
        "coverage": "cross-env BABEL_ENV=test jest --coverage",
        "clean": "rimraf ./dist",
        "build:style": "grunt",
        "watch:style": "grunt watch",
        "build:pub": "npm run clean && npm run build:webpack && npm run build:ts && npm run build:style",
        "pub:dev": "yarn build:pub && npm publish --tag=dev --registry=http://registry.m.jd.com",
        "pub:beta": "yarn build:pub && npm publish --tag=beta --registry=http://registry.m.jd.com ",
        "pub:prod": "yarn build:pub && npm publish --registry=http://registry.m.jd.com"
